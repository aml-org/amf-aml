#%Dialect 1.0
dialect: Test
version: "1.0"
nodeMappings:
  Local:
    classTerm: v3.Local
    mapping:
      a:
        patch: insert
        unique: true
        mandatory: true
        propertyTerm: v3.a
        range: string
  myDeclarations:
    classTerm: v3.Decla
    mapping:
      b:
        mandatory: true
        propertyTerm: v3.b
        range: string
documents:
  root:
    encodes: Local
  library:
    declares:
      declarations: myDeclarations
  options:
    declarationsPath: declarations/path
    referenceStyle: RamlStyle
external:
  v3: http://test.com/v3#
