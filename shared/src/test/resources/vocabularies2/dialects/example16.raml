#%Dialect 1.0
dialect: Test
version: "1.0"
nodeMappings:
  Local:
    classTerm: v3.Local
    mapping:
      a:
        unique: true
        mandatory: true
        propertyTerm: v3.a
        range: string
      b:
        unique: true
        mandatory: true
        propertyTerm: v3.b
        range: string
      c:
        propertyTerm: v3.c
        range: string
    idTemplate: "#/{a}_{b}"
documents:
  root:
    encodes: Local
external:
  v3: http://test.com/v3#
